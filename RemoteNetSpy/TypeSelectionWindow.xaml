<Window x:Class="RemoteNetSpy.TypeSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RemoteNetSpy"
        mc:Ignorable="d"
        Title="Type Selection"
        Height="450"
        Width="800">
    <Grid>
        <local:TypesControl x:Name="TypesControl" />
        <Button Content="OK" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="75" Click="OkButton_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="75" Click="CancelButton_Click"/>
    </Grid>
</Window>
